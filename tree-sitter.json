{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "orca",
      "camelcase": "Orca",
      "title": "Orca",
      "scope": "source.orca",
      "file-types": [
        "inp"
      ],
      "injection-regex": "^orca$",
      "class-name": "TreeSitterOrca"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for input file for the Orca quantum chemistry package",
    "authors": [
      {
        "name": "Kalman Szenes",
        "email": "szenes.kalman@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-orca"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}